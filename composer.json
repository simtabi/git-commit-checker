{
  "name": "simtabi/git-commit-checker",
  "description": "Check coding standard & code syntax with Git pre-commit hook.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "simtabi",
    "simtabi cms",
    "simtabi platform",
    "simtabi git commit checker"
  ],
  "homepage": "https://simtabi.com",
  "authors": [
    {
      "name": "Imani Manyara",
      "email": "imani@simtabi.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Simtabi\\GitCommitChecker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Simtabi\\GitCommitChecker\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.1.3|^8.0",
    "laravel/framework": "^5.6|^6|^7|^8",
    "php-parallel-lint/php-parallel-lint": "^1.0",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1|^7.0|^8.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Simtabi\\GitCommitChecker\\Providers\\GitCommitCheckerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
